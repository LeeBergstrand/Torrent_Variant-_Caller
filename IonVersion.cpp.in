string IonVersion::GetFullVersion(std::string module)
{
	string v = module;
	v += " (";
	v += "4";
	v += ".";
	v += "4";
	v += ".";
	v += "3";
	v += ")\n";
	v += "Build: ";
	v += "";
	v += " (";
	v += "";
	v += ")\nSource: ";
	v += "";
	v += " (";
	v += "b4969eb-2479794";
	v += ")\n";
	return v;
}

